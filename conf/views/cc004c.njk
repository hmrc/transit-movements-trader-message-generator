<ncts:CC004C PhaseID="NCTS5.0" xmlns:ncts="http://ncts.dgtaxud.ec">
  <messageRecipient>{{ messageRecipient }}</messageRecipient>
  <preparationDateAndTime>{{ preparationDateAndTime }}</preparationDateAndTime>
  <messageIdentification>{{ messageIdentification }}</messageIdentification>
  <messageType>{{ messageType }}</messageType>
  {#- Optional: #}
  {%- for i in range(0, 0) %}
  <correlationIdentifier>{{ correlationIdentifier }}</correlationIdentifier>
  {%- endfor %}
 <TransitOperation>
    {#- Optional: #}
    {%- for i in range(0, 0) %}
    <MRN>{{ MRN }}</MRN>
    {%- endfor %}
    {#- Optional: #}
    {%- for i in range(0, 0) %}
    <LRN>{{ LRN }}</LRN>
    {%- endfor %}
    <amendmentSubmissionDateAndTime>{{ amendmentSubmissionDateAndTime }}</amendmentSubmissionDateAndTime>
    <amendmentAcceptanceDateAndTime>{{ amendmentAcceptanceDateAndTime }}</amendmentAcceptanceDateAndTime>
  </TransitOperation>
  <CustomsOfficeOfDeparture>
    <referenceNumber>{{ referenceNumberDeparture }}</referenceNumber>
  </CustomsOfficeOfDeparture>
  <HolderOfTheTransitProcedure>
    {#- Optional: #}
    {%- for i in range(0, 0) %}
    <identificationNumber>{{ identificationNumber }}</identificationNumber>
    {%- endfor %}
    {#- Optional: #}
    {%- for i in range(0, 1) %}
    <TIRHolderIdentificationNumber>{{ TIRHolderIdentificationNumber }}</TIRHolderIdentificationNumber>
    {%- endfor %}
    {#- Optional: #}
    {%- for i in range(0, 0) %}
    <name>{{ name }}</name>
    {%- endfor %}
    {#- Optional: #}
    {%- for i in range(0, 0) %}
    <Address>
        <streetAndNumber>{{ streetAndNumber }}</streetAndNumber>
        {#- Optional: #}
        {%- for i in range(0, 0) %}
        <postcode>{{ postcode }}</postcode>
        {%- endfor %}
        <city>{{ city }}</city>
        <country>{{ country }}</country>
    </Address>
    {%- endfor %}
  </HolderOfTheTransitProcedure>
</ncts:CC004C>
